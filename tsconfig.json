{
    "compilerOptions": {
        "target": "es5",
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "buildjs",
        "removeComments": true,
        "declaration": false,
        "sourceMap": false,
        "importHelpers": true,
        "noImplicitAny": true,
        "lib": [
          "ES5",
          "ES2015",
          "DOM",
          "ScriptHost"
        ]
    },
    "exclude": [
        "node_modules",
        "test"
    ]
}