{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "es5",
            "es2015.promise",
            "dom"
        ],
        "noUnusedLocals": true,
        "strictNullChecks": true,
        "module": "commonjs",
        "rootDir": "lib",
        "outDir": "buildjs",
        "removeComments": true,
        "declaration": false,
        "sourceMap": false,
        "noImplicitAny": true
    },
    "files": [
        "typings/index.d.ts",
        "lib/typings/index.d.ts",
        "lib/webcrypto.ts"
    ]
}